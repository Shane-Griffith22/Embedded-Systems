#include <stdint.h>

#define RCC_BASE_ADDR 			0x40023800UL

#define RCC_CFGR_OFFSET			0X08

#define RCC_CFGR_ADDR 			(RCC_BASE_ADDR + RCC_CFGR_OFFSET)

#define RCC_GPIOA_OFFSET		0x30

#define RCC_AHB1_EN_REG			(RCC_BASE_ADDR + RCC_GPIOA_OFFSET)

int main(void)
{

uint32_t *pRccAhb1Enr = (uint32_t) RCC_AHB1_EN_REG;

//enable gpioA on  AHHB1 Bus
*pRccAhb1Enr |= 1 << 0;


    /* Loop forever */
	for(;;);
}
