#include <stdint.h>

#define RCC_BASE_ADDR 			0x40023800UL

#define RCC_CFGR_OFFSET			0X08

#define RCC_CFGR_ADDR 			(RCC_BASE_ADDR + RCC_CFGR_OFFSET)

#define RCC_GPIOA_OFFSET		0x30

#define RCC_AHB1_EN_REG			(RCC_BASE_ADDR + RCC_GPIOA_OFFSET)

#define GPIOA_BASE_ADDR			0x40020000UL

#define RCC_CLK_CFGR_OFFSET		0x08

#define RCC_CLK_CFGR_REG		(0x40023800UL + 0x08)

#define GPIOA_AFRH_OFFSET		0x24

#define GPIOA_AFRH				(GPIOA_BASE_ADDR + GPIOA_AFRH_OFFSET)



int main(void)
{

uint32_t *pRccAhb1Enr = (uint32_t) RCC_AHB1_EN_REG;

//enable gpioA on  AHHB1 Bus
*pRccAhb1Enr |= 1 << 0;

//configure gpioA pin 8 for alternate function
uint32_t *pGpioaConfig = (uint32_t) GPIOA_BASE_ADDR;
*pGpioaConfig &= ~(0x3 << 16);
*pGpioaConfig |= (0x2 << 16);


//configure mco1 to use hsi clock
uint32_t *pRccClkCfgr = (uint32_t) RCC_CLK_CFGR_REG;
*pRccClkCfgr &= ~(0x3 << 21);

uint32_t *pGpioaAfrh = (uint32_t) GPIOA_AFRH;
*pGpioaAfrh &= ~(0xf << 0);



    /* Loop forever */
	for(;;);
}
